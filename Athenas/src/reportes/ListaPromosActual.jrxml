	<queryString language="SQL">
		<![CDATA[SELECT dbo."Promo"."ID",
	dbo."Producto"."Descripcion" AS Producto,
	dbo."Promo"."Tipo",
	dbo."Promo"."Valor",
	dbo."Promo"."FecIni",
	dbo."Promo"."FecFin"
FROM dbo."Promo"
	inner join dbo."Producto" ON 
	 dbo."Producto"."ID" = dbo."Promo"."Cod_Prod" 
WHERE 
	 convert(varchar,GETDATE(),101) BETWEEN dbo."Promo"."FecIni" AND dbo."Promo"."FecFin"]]>
